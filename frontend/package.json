{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "type-check": "vue-tsc --noEmit",
    "lint": "biome lint src vite.config.ts",
    "lint:fix": "biome lint --write src vite.config.ts",
    "format": "biome format --write src vite.config.ts",
    "format:check": "biome format src vite.config.ts",
    "lint:dead-code": "knip"
  },
  "dependencies": {
    "echarts": "^6.0.0",
    "lucide-vue-next": "^0.563.0",
    "monaco-editor": "^0.55.1",
    "vue": "^3.5.24",
    "vue-echarts": "^8.0.1",
    "vue-router": "^4.6.4",
    "vue3-grid-layout-next": "^1.0.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "happy-dom": "^20.4.0",
    "knip": "^5.64.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.1.4"
  }
}
